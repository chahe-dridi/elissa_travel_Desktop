<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="1200.0" styleClass="anchor-pane-background" stylesheets="@airport.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.elissa.Controller.ReservationVolController">

    <Button fx:id="backButton" layoutX="1000.0" layoutY="140.0" onAction="#handleBackButtonClick" prefHeight="40.0" prefWidth="160.0" text="Return" />

    <AnchorPane layoutX="128.0" layoutY="189.0" prefHeight="563.0" prefWidth="941.0">
        <TableView fx:id="reservationTable" layoutX="-59.0" layoutY="35.0" prefHeight="550.0" prefWidth="1000.0">
            <columns>


                <TableColumn fx:id="totalPriceColumn" prefWidth="133.0" text="Total Price" />
                <TableColumn fx:id="paymentMethodColumn" prefWidth="155.0" text="Payment Method" />


                <TableColumn fx:id="descriptionColumn" prefWidth="178.0" text="Description" />
                <TableColumn fx:id="classNameColumn" prefWidth="150.0" text="Class Name" />
                <TableColumn fx:id="departureTimeColumn" prefWidth="168.0" text="Departure Time" />
                <TableColumn fx:id="arrivalTimeColumn" prefWidth="150" text="Arrival Time" />
                <TableColumn fx:id="departureAirportColumn" prefWidth="150" text="Departure Airport" />
                <TableColumn fx:id="arrivalAirportColumn" prefWidth="150" text="Arrival Airport" />
                <TableColumn fx:id="cityColumn" prefWidth="100" text="City" />
                <TableColumn fx:id="countryColumn" prefWidth="100" text="Country" />


            </columns>
        </TableView>
    </AnchorPane>

</AnchorPane>
